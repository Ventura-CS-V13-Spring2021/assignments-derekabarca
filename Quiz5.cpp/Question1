#include <iostream>
using namespace std;

int main()
{
    const int SIZE=10;
    int     numbers[10] = {5, 10, 75, 80, 25, 55, 95, 70, 60, 15};
    int     diff[10];
    int     sum=0;

    //*** Your code
    //print and sum the elements in the array
    for(int i=0; i<SIZE; i++)
    {
        sum += numbers[i];
        cout << numbers[i] << "\t";
    }
    cout << endl;
    for (int i=0; i<SIZE; i++)
    {
        diff[10] -= numbers[i];
        //cout << numbers[i] - numbers[10] << "\t";
        cout << numbers[i] << "\t";
    }
    cout << endl << "Sum of array elements is: " << sum << endl;

}
