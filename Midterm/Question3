int getRdnum ();
{
    srand(time(0));

    for (int x = 1; x <= 3; x++){
        cout << 1 + (rand() % 9) << endl;
        
    }
}

int findMin(num1, num2, num3);{
    if (num1 < num2 && num1 < num3){
        cout << num1 << endl;
    }
    if (num2 < num1 && num2 < num3);
    cout << num2 << endl;
    if (num3 < num2 && num3 < num1){
        cout << num3 << endl;
    }
}

int findMax ();{

    if (num1 > num2 && num1 > num3){
        cout << num1 << endl;
    }
    if (num2 > num1 && num2 > num3){
    cout << num2 << endl;
    }
    if (num3 > num2 && num3 > num1){
        cout << num3 << endl;
    }
}

